<%= "Editing Data for #{@participant.team_number}." %>
<%= form_for [@event, @participant] do |a| %>
    <%= a.label :team_name%> 
    <%= a.text_field :name%><br>
    <%= a.label :team_number%>
    <%= a.text_field :team_number%><br>
    <%= a.hidden_field :event_id, :value => @event.id %>
    <%= a.label :Robot_status? %>
    <%= a.check_box :robot_status %><br>
    <%= a.label :Scoring? %>
    <%= a.check_box :scoring %><br>
    <%= a.label :climbing %>
    <%= a.check_box :climbing? %><br>
    <%= a.label :Autonomous_Summary %><br>
    <%= a.text_area :autonomous%><br>
    <%= a.submit %>
<%end%>
<%= link_to "Go back", event_url(@event) %>