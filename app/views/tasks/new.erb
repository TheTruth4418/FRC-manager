<p> New Task </p>
<%= form_for [Task.new] do |a| %>
    <%= a.label :task_name%> 
     <%= a.text_field :name%><br>
     <%= a.label :user_id %>
     <%= a.collection_select :user_id, @students, :id, :username, include_blank: true %> <br>
     <%= a.label :task_description%><br>
     <%= a.text_area :description %> <br>
     <%= a.hidden_field :complete, :value => 0 %>
     <%= a.hidden_field :team_id, :value => @team.id %> 
     <%= a.submit %>
     <%end%>
     <%= link_to "Go Back", team_url(@team)%>