<%="Scouting report for #{@participant.name}" %>
<%= form_for [@event,Scout.new] do |a| %>
    <%= a.label :Completed_robot? %>
    <%= a.check_box :robot %><br>
    <%= a.label :Scoring? %>
    <%= a.check_box :scoring %><br>
    <%= a.label :climbing %>
    <%= a.check_box :climbing? %><br>
    <%= a.label :Autonomous_Summary %><br>
    <%= a.text_area :autonomus%>
     <%= a.hidden_field :participant_id, :value => @participant.id %> 
     <%= a.submit %>
     <%end%>
     <%= link_to "Go Back", team_url(@team)%>
